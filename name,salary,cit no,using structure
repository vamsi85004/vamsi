#include <stdio.h>
#include <string.h>
struct Person {
  char name[50];
  int citNo;
  float salary;
} person1;
int main() {
  strcpy(person1.name, "VAMSI KOTI REDDY.SIDDAVATAM");
  person1.citNo = 192211977;
  person1. salary = 150000;
  printf("Name: %s\n", person1.name);
